{
  "name": "@khanonjs/engine",
  "version": "0.1.31",
  "description": "A typescript game engine extending Babylon.js",
  "private": false,
  "author": {
    "name": "Lorenzo Portillo"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Khanon-js/khanon.js.git"
  },
  "keywords": [
    "typescript",
    "game",
    "gamedev",
    "engine",
    "babylonjs",
    "javascript"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && cpy package.json ./dist && npm run cpy-declarations && cpy ./README.md ./dist",
    "cpy-declarations": "run-script-os",
    "cpy-declarations:win32": "cpy ./src/**/*.d.ts ./dist",
    "cpy-declarations:default": "cd src && find . -name '*.d.ts' | cpio -pumd ../dist",
    "build:docs": "typedoc --options typedoc.json",
    "publish:docs": "node ./publish/publish-docs.mjs",
    "publish:validate-version": "node ./publish/validate-version.js",
    "start:docs": "http-server ./api-docs -o",
    "test": "jest",
    "link": "cpy package.json ./src && cd ./src && npm link && npm i",
    "unlink": "cd ./src && npm uninstall --global && rimraf node_modules && rimraf package.json && rimraf package-lock.json",
    "clean": "rimraf dist",
    "webpack-bundle-analyze": "webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json",
    "circular-refs": "dpdm ./src/index.ts",
    "check-updates": "npm-check-updates -u",
    "lint": "eslint ./src/**/*.*",
    "lint:fix": "eslint ./src/**/*.* --fix"
  },
  "devDependencies": {
    "@babylonjs/core": "~7.50.0",
    "@babylonjs/gui": "~7.50.0",
    "@babylonjs/gui-editor": "~7.50.0",
    "@babylonjs/inspector": "~7.50.0",
    "@babylonjs/loaders": "~7.50.0",
    "@babylonjs/materials": "~7.50.0",
    "@babylonjs/serializers": "~7.50.0",
    "@jest/globals": "^29.7.0",
    "@mxssfd/typedoc-theme": "file:mxssfd-typedoc-theme",
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.5.10",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "chai": "^4.3.10",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "dpdm": "^3.14.0",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "form-data": "^4.0.0",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "npm-check-updates": "^16.14.11",
    "recursive-copy": "^2.0.14",
    "rimraf": "^5.0.5",
    "run-script-os": "^1.1.6",
    "semver": "^7.5.4",
    "sinon": "^17.0.1",
    "string-replace-loader": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "@babylonjs/core": "~7.50.0",
    "@babylonjs/gui": "~7.50.0",
    "@babylonjs/inspector": "~7.50.0",
    "@babylonjs/loaders": "~7.50.0",
    "@babylonjs/materials": "~7.50.0",
    "@babylonjs/serializers": "~7.50.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  }
}