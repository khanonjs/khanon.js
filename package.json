{
  "name": "@khanonjs/engine",
  "version": "0.0.36",
  "description": "A typescript game engine extending Babylon.js",
  "private": false,
  "author": {
    "name": "Lorenzo Portillo"
  },
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Khanon-js/khanon.js.git"
  },
  "main": "index.ts",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && cpy package.json ./dist",
    "build:docs": "typedoc --options typedoc.json",
    "publish:docs": "node ./publish/publish-docs.mjs",
    "publish:validate-version": "node ./publish/validate-version.js",
    "test": "jest",
    "link": "cpy package.json ./src && cd ./src && npm link && npm i",
    "unlink": "cd ./src && npm uninstall --global && rimraf node_modules && rimraf package.json && rimraf package-lock.json",
    "clean": "rimraf dist",
    "webpack-bundle-analyze": "webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json",
    "check-circular-refs": "dpdm ./src/index.ts",
    "check-updates": "npm-check-updates -u",
    "lint": "eslint ./src/**/*.*",
    "lint:fix": "eslint ./src/**/*.* --fix"
  },
  "devDependencies": {
    "@babylonjs/core": "~6.38.0",
    "@babylonjs/gui": "~6.38.0",
    "@babylonjs/gui-editor": "~6.38.0",
    "@babylonjs/inspector": "~6.38.0",
    "@babylonjs/loaders": "~6.38.0",
    "@babylonjs/materials": "~6.38.0",
    "@babylonjs/serializers": "~6.38.0",
    "@jest/globals": "^29.7.0",
    "@mxssfd/typedoc-theme": "file:mxssfd-typedoc-theme",
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.4",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "chai": "^4.3.10",
    "clean-webpack-plugin": "^4.0.0",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "form-data": "^4.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "npm-check-updates": "^16.14.11",
    "recursive-copy": "^2.0.14",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "semver": "^7.5.4",
    "sinon": "^17.0.1",
    "string-replace-loader": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.3",
    "typescript": "^5.5.0-dev.20240221",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "@babylonjs/core": "~6.38.0",
    "@babylonjs/gui": "~6.38.0",
    "@babylonjs/inspector": "~6.38.0",
    "form-data": "~4.0.0"
  },
  "peerDependenciesMeta": {
    "form-data": {
      "optional": true
    }
  }
}
